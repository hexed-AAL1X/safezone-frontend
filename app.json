{
  "expo": {
    "name": "SafeZone: V1.0",
    "slug": "safezone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "safezone",
    "userInterfaceStyle": "dark",
    "description": "SafeZone - Tu seguridad personal 24/7. Alertas SOS, ubicaci√≥n en tiempo real y comunidad de seguridad.",
    "splash": {
      "image": "./assets/images/logo.png",
      "backgroundColor": "#000000",
      "resizeMode": "contain"
    },
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aal1x.safezonefinalahorasi"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.aal1x.safezonefinalahorasi",
      "versionCode": 1,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "RECORD_AUDIO",
        "READ_CONTACTS"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-location",
      "expo-font",
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsVersion": "11.15.2"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "235198b0-78b3-44bc-bc63-190a6dcda49a"
      }
    }
  }
}
